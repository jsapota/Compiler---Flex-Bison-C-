VAR

a b c d

BEGIN

a := 0;
b := 0;
c := 0;
d := 0;

WHILE a < b DO
    WHILE b < cDO
        WHILEc<dDO
            a:=b+c;
        ENDWHILEENDWHILEENDWHILE { tu ma byc dobrze }

WHILEa<bDOSKIP;ENDWHILE { tu tez dobrze }

{ tu blad }
WHILE a < b / d DO
    SKIP;
ENDWHILE

END
