{
    ERROR!
    iterator is redeclared: i
    LINE: 20                       FOR i FROM j TO 10000 DO
}

VAR

a b c

BEGIN

READ a;
READ b;
READ c;

FOR i FROM 1 TO 10000 DO
    a := b + c;
    FOR j FROM 1 TO 10000 DO
        FOR i FROM j TO 10000 DO
            SKIP;
        ENDFOR
    ENDFOR
ENDFOR

END
