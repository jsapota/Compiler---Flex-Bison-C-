EX72
chodzi dokladnie o nastepujace dzielenie
b := 18;

a := 100000 / b;
WRITE a;
jezeli zamiast tego damy
c := 100000;
a := c /b
WRITE a; 

to juz dziala normalnie







EX74
VAR

a b c

BEGIN

b := 16;

Jezeli wywalimy ten kawalek to dziala normalnie
a := 100;
c := a / 8;
WRITE c;

c := a / b;
WRITE c;
kawalek do tego miejsca

READ a;
c := a / 8;
WRITE c;

c := a / b;
WRITE c;

READ a;
READ b;

c := a / b;
WRITE c;

END



